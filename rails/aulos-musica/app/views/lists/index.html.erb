<h1>Lists#index</h1>
<p>Find me in app/views/lists/index.html.erb</p>
<table border="1" cellspacing="5" cellpadding="5">
	<tr>
		<% @lists.attribute_names.each do |list| %>
			<th><%=list%></th>
		<%end%>
	</tr>
	<% @lists.each do |list| %>
		<tr>
			<%list.attributes.each_value do |v|%>
				<td><%=v%></td>
			<%end%>
		</tr>
	<%end%>
	
</table>