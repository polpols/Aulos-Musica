<h1>Lists#index</h1>
<p id='notice'>
<%= notice %>
</p>
<%= link_to "New List", new_list_path() %>
<table cellspacing="5" cellpadding="5">
	<thead>
		<tr>
			<% @lists.attribute_names.each do |list| %>
				<th><%=list%></th>
			<%end%>
		</tr>
	</thead>
	<tbody>

		<% @lists.each do |list| %>
			<tr>
				<%list.attributes.each_value do |v|%>
					<td><%=v%></td>
				<%end%>
				<td>
					<%= link_to	"liste", list_path(list) %>
				</td>
				<td>
					<%= list.products_count %>
				</td>
			</tr>
		<%end%>
	</tbody>
</table>
